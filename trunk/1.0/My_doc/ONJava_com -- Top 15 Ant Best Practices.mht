  =
[style=3Ddotted];<BR>&lt;/xsl:text&gt;<BR>&lt;/xsl:template&gt;<BR><BR>&l=
t;xsl:template=20
  match=3D"target"&gt;<BR>&lt;xsl:choose&gt;<BR>&lt;xsl:when=20
  test=3D"@depends"&gt;<BR>&lt;xsl:call-template=20
