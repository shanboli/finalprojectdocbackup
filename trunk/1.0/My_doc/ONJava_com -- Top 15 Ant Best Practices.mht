  =
select=3D"project/target//ant"/&gt;<BR><BR>&lt;xsl:text&gt;}<BR>&lt;/xsl:=
text&gt;<BR>&lt;/xsl:template&gt;<BR><BR>&lt;!--=20
  show antcall tasks with dotted lines --&gt;<BR>&lt;xsl:template=20
  match=3D"antcall"&gt;<BR>&lt;xsl:text&gt; =
"&lt;/xsl:text&gt;<BR>&lt;xsl:value-of=20
  select=3D"ancestor::target/@name"/&gt;<BR>&lt;xsl:text&gt;" -&gt;=20
  "&lt;/xsl:text&gt;<BR>&lt;xsl:value-of=20
  select=3D"@target"/&gt;<BR>&lt;xsl:text&gt;"=20
  =
[style=3Ddotted];<BR>&lt;/xsl:text&gt;<BR>&lt;/xsl:template&gt;<BR><BR>&l=
t;!--=20
  show ant file tasks with dotted lines --&gt;<BR>&lt;xsl:template=20
  match=3D"ant"&gt;<BR>&lt;xsl:text&gt; =
"&lt;/xsl:text&gt;<BR>&lt;xsl:value-of=20
  =
select=3D"@antfile"/&gt;<BR>&lt;xsl:text&gt;:&lt;/xsl:text&gt;<BR>&lt;xsl=
:value-of=20
  select=3D"@target"/&gt;<BR>&lt;xsl:text&gt;"=20
  [shape=3Dbox];<BR>&lt;/xsl:text&gt;<BR><BR>&lt;xsl:text&gt;=20
  "&lt;/xsl:text&gt;<BR>&lt;xsl:value-of=20
